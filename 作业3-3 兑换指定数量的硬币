#include <stdio.h>
#include <math.h>
int main() {
    int n;
    scanf("%d",&n);
    int count = 0;
    for (int i = 100; i >=1 ; --i) {

        for (int j = 100; j >=1 ; --j) {

            for (int k = 100; k >=1 ; --k) {

                if(i*5+j*2+k*1==180&&i+j+k==100&&count<n){
                    count++;
                    printf("%d %d %d\n",i,j,k);
                }

            }

        }
    }
    return 0;
}
