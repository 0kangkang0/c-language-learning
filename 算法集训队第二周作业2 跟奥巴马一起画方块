#include <stdio.h>
int main(){
    int n;
    char ch;
    scanf("%d %c",&n,&ch);
    double x=n*1.0/2-n/2;
    int b;
    if(x>=0.5){
        b=n/2+1;
    } else{
        b=n/2;
    }
    for (int i = 0; i < b; ++i) {
        for (int j = 0; j < n; ++j) {
            printf("%c",ch);
        }
        printf("\n");
    }
    return 0;
}
